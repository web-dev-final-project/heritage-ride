<div class="search-bar-wrapper shadow d-flex justify-content-center">
  <div class="search-bar">
    {{> searchBar}}
  </div>
</div>
<div class="search-main">
  {{#if error}}
  <p style="color: red">{{ error }}</p>
  {{/if}}
  {{#if results.length}}
  <ul class="list-group container card p-3 search-grid" id="search-grid"></ul>
  {{else}}
  <p id="search-not-found" class="fs-4">
    No Item Found Matching Your Criteria.
  </p>
  {{/if}}
</div>

<a href="/">Return to Homepage</a>

<script>
  const listings = {{{json results}}}
  console.log(listings)
  {{#if user}}
    const user = {{{json user}}};
  {{else}}
    const user = null;
  {{/if}}
</script>
<script src="/scripts/search.js"></script>
