<div id="seller-central-page">
  <p class="fs-3">Welcome back, {{ user.firstName }}!</p>
  <p class="">
    There is <span id="listing-attention-num">0</span> listings require your
    attention.
  </p>
  {{#if listings}}
  <div>
    <ul>
      <h3>Car Listings</h3>
    </ul>
    <ul>
      <h3>Part Listings</h3>
    </ul>
  </div>
  {{else}}
  <p class="fs-4">
    You don't have anything listed, lets start by add a new listing.
  </p>
  {{/if}}
  <button class="btn btn-primary" id="add-listing">Add Listing</button>
</div>

<div id="seller-add-page" class="d-none">{{> addListingGuide}}</div>

<script></script>
<script src="/scripts/seller.js"></script>
